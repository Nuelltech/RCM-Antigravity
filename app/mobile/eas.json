{
    "cli": {
        "version": ">= 5.9.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EAS_SKIP_AUTO_FINGERPRINT": "1",
                "EXPO_PUBLIC_API_URL": "https://rcm-nuelltech.onrender.com"
            }
        },
        "staging": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EAS_SKIP_AUTO_FINGERPRINT": "1",
                "EXPO_PUBLIC_API_URL": "https://rcm-antigravity-staging.onrender.com"
            }
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {},
        "staging": {
            "android": {
                "track": "internal"
            }
        }
    }
}