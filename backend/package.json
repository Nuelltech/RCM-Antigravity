{
  "name": "rcm-backend",
  "version": "1.0.0",
  "description": "RCM Backend",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "src/core/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/core/server.ts",
    "build": "npx prisma generate && tsc",
    "start": "node dist/core/server.js",
    "prisma:generate": "npx prisma@5.10.2 generate",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:vision": "tsx scripts/test-vision-api.ts",
    "backup": "tsx scripts/quick-backup.ts",
    "backup:data": "tsx scripts/backup_data.ts",
    "restore": "tsx scripts/restore_data.ts",
    "seed": "prisma db seed",
    "seed:produtos": "tsx scripts/seed-produtos.ts",
    "seed:complementares": "tsx scripts/seed-produtos-complementares.ts",
    "seed:receitas": "tsx scripts/seed-receitas.ts",
    "seed:subfamilias-bebidas": "tsx scripts/seed-subfamilias-bebidas.ts",
    "seed:formatos-venda": "tsx scripts/seed-formatos-venda.ts",
    "seed:subscriptions": "tsx scripts/seed-subscription-plans.ts",
    "seed:internal-users": "tsx scripts/seed-internal-users.ts",
    "seed:load-testing": "tsx scripts/seed-load-testing.ts",
    "seed:all": "tsx scripts/seed.ts && tsx scripts/seed-produtos.ts && tsx scripts/seed-produtos-complementares.ts && tsx scripts/seed-receitas.ts && tsx scripts/seed-subfamilias-bebidas.ts && tsx scripts/seed-formatos-venda.ts",
    "migrate:codes": "tsx scripts/migrate-product-codes.ts",
    "migrate:data": "tsx scripts/migrate-data.ts",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "worker:recalc": "tsx src/workers/recalculation-worker.ts",
    "start:worker": "tsx src/workers/index.ts",
    "start:worker:prod": "node dist/workers/index.js"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@google-cloud/vision": "^4.0.2",
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "5.10.2",
    "@react-email/components": "^0.0.14",
    "@react-email/render": "^0.0.12",
    "@sendgrid/mail": "^8.1.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.48",
    "basic-ftp": "^5.0.5",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "expo-server-sdk": "^3.10.0",
    "fastify": "^4.26.1",
    "fastify-type-provider-zod": "^1.1.9",
    "fuse.js": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.8",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "prisma": "5.10.2",
    "react": "^18.2.0",
    "tesseract.js": "^5.0.5",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0"
  },
  "prisma": {
    "seed": "ts-node scripts/seed.ts"
  }
}
